<html>
 <head>
   
  <script type="text/javascript">
 document.write("<h1>Ch 5-Lab Exercise 7</h1><hr>");
   numbers = new Array();
   var a=parseInt(prompt('Enter First number '));
   var b=parseInt(prompt('Enter Second number'));
   var c=parseInt(prompt('Enter Third number'));
   var d=parseInt(prompt('Enter Fourth number'));
   var e=parseInt(prompt('Enter Fifth number'));
   numbers.push(a,b,c,d,e);
   document.write("Your entered numbers are - </br>");	
   for(i=0; i<numbers.length; i++) 
   {
	document.write(numbers[i]+"</br>");
   }

   var temp = numbers[1]; 	
   for(i=0; i<numbers.length; i++) 
   {	
	if(numbers[i]<temp)
           temp = numbers[i];
   }
   document.write("<br> Minimum number of all above numbers is - "+ temp+"</br>");	  
   for(i=0; i<numbers.length; i++) 
   {	
       if(numbers[i]>temp)
           temp = numbers[i];
   }
   document.write("<br> Maximum number of all above numbers is - "+ temp+"</br>");	  
  </script>
 </head>
<body>
</body>
</html>
